{
  "name": "@mrsubtle/oag-express-marketplace",
  "version": "0.2.1",
  "description": "Embeddable React OpticAg Marketplace with express-checkout for SaaS and partner platforms.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrsubtle/oag-express-marketplace.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --inject-style",
    "dev": "tsup src/index.tsx --watch --inject-style",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.12",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@medusajs/icons": "^2.8.7",
    "@medusajs/js-sdk": "^2.8.7",
    "@medusajs/types": "^2.8.7",
    "@medusajs/ui": "^4.0.17",
    "@medusajs/ui-preset": "^2.8.7",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "tailwind-merge": "^3.3.1"
  }
}
